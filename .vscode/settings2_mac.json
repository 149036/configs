  // verilog/systemverilog/vhdl/hdl
  "verilog.linting.linter": "verilator",
  "verilog.linting.iverilog.runAtFileLocation": true,
  "arduino.useArduinoCli": true,
  "arduino.additionalUrls": [],
  "arduino.logLevel": "info",
